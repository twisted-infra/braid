.twisted
    .load-indicator(ng-show='c.loading')
        .spinner
            img(src="/twisted_view/img/twisted.png")
            p loading, maybe, someday
    .header
        .header-content(ng-style='{width: c.widthInclusive}')
            .col-10
                p Builders
            .col-90(ng-style="{'width': c.width}")
                twisted-builders-header(builders='c.builders' width='c.width' cell-width='c.cellWidth')
    .content
        .twisted-change-rows(ng-style='{width: c.widthInclusive}')
            twisted-change-row(ng-repeat="change in c.changes | filter: search | orderBy: 'changeid':true track by change.changeid"
                       change='change'
                       tag='tag'
                       builders='c.builders'
                       width='c.width'
                       cell-width='c.cellWidth')
